{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@astral-mines/*": ["./src/astral-mines/*"],
      "@tic-tac-morph/*": ["./src/tic-tac-morph/*"]
    }
  }
}
